{
  "extensions": ["tangelo-document", "tangelo-paragraph", "tangelo-text", "tangelo-introduction"],
  "inlineExtensions": {
    "doc": { "content": "introduction? paragraph*" }
  },
  "configuration": {},
  "type": "doc",
  "content": [
    {
      "type": "introduction",
      "content": [{ "type": "paragraph", "content": [{ "type": "text", "text": "Some introductory words..." }] }]
    },
    {
      "type": "paragraph",
      "content": [{ "type": "text", "text": "It's a dynamic world!" }]
    }
  ]
}
